public class Searching{
    public static void main(String[] args){
        int[] arr={3,4,7,1,4,8,9};
        System.out.println(min(arr));

    }
    static int min(int[] arr){
        int ans=arr[0];
        for(int i =0;i<arr.length;i++){
        if(arr[i]<arr[0]){
            ans = arr[i];

        }
        
    }
    return ans;
}
}

